{
  "manifest_version": 2,

  "name": "Sethome",
  "description": "Personalized Google Chrome Homepage",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ],
   "chrome_url_overrides" : {
    "newtab": "test.html"
  }
}
